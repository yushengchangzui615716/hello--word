#include <stdio.h>

int main()
{
        int a = 520;
        char b = 'F';
        float c = 3.14;
        double d = 3.141592653;

        printf("鱼C工作室创办于2010年的%d\n", a);
        printf("I love %cishC.com!\n", b);
        printf("圆周率是：%.2f\n", c);
        printf("精确到小数点后9位的圆周率是：%11.9f\n", d);

        return 0;
}

                              //enum（枚举关键字）       //枚举常量-一一列举
#include <stdio.h> 
enum xingbie
{
   nan,
   nv,                                  //枚举常量（具有常亮的性质） 
   baomi
} ;                                       //性别：男 女  保密 
int main()
{
  //enum xingbie fu=nan
  printf("%d\n",nan); 
   printf("%d\n",nv);
    printf("%d\n",baomi);
  return 0;
}
 
 
